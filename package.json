    "react": "16.6.0",
    "react-dom": "16.6.0",
    "react-helmet": "5.2.0",
    "react-router-dom": "4.3.1"
  },
  "devDependencies": {
    "http-proxy-middleware": "^0.19.0",
    "prettier": "1.11.1",
    "react-scripts": "2.1.1",
    "react-test-renderer": "16.6.0",
    "react-testing-library": "5.2.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
